       </div>\r\n\r\n        <div class=\"qualities-grid\">\r\n          <div v-for=\"(quality, index) in profile.qualities\" \r\n               :key=\"quality\"\r\n               class=\"quality-item\"\r\n               :style=\"{ animationDelay: `${index * 0.1}s` }\">\r\n            <div class=\"quality-dot\"></div>\r\n            <span class=\"quality-text\">{{ quality }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section id=\"projects\" class=\"section projects\">\r\n      <div class=\"container\">\r\n        <h2 class=\"section-title\">精选项目</h2>\r\n        <h3 class=\"section-subtitle\">实际项目成果</h3>\r\n        <p class=\"section-description\">看看我是如何将概念转化为引人入胜的数字体验。</p>\r\n        \r\n        <div class=\"grid grid-3\">\r\n          <div v-for=\"project in featuredProjects\"\r\n               :key=\"project.id\"\r\n               class=\"card project-card\">\r\n            <div class=\"project-year\">{{ project.company || 'Personal' }}•{{ new Date().getFullYear() }}</div>\r\n            <h3 class=\"project-title\">{{ project.title }}</h3>\r\n            <div class=\"project-divider\"></div>\r\n            <p class=\"project-description\">{{ project.description }}</p>\r\n            <div class=\"project-tags\">\r\n              <span v-for=\"tag in project.tags\"\r\n                    :key=\"tag\"\r\n                    class=\"tag\">\r\n                {{ tag }}\r\n              </span>\r\n            </div>\r\n            <div class=\"project-links\">\r\n              <a :href=\"project.github\" class=\"project-link\" target=\"_blank\">\r\n                <i class=\"icon-github\"></i>\r\n              </a>\r\n              <a :href=\"project.link\" class=\"project-link\" target=\"_blank\">\r\n                <i class=\"icon-external-link\"></i>\r\n              </a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"testimonials section\">\r\n      <div class=\"container\">\r\n        <h2 class=\"section-title\">客户评价</h2>\r\n        <h3 class=\"section-subtitle\">他们如何评价我的工作</h3>\r\n        <p class=\"section-description\">不要只相信我的话，看看我的客户是如何评价我的工作的。</p>\r\n        \r\n        <div class=\"testimonials-container\" ref=\"testimonialsContainer\">\r\n          <div class=\"testimonials-track\" \r\n               :style=\"{ transform: `translateX(-${currentTestimonial * 100}%)` }\">\r\n            <div v-for=\"testimonial in profile.testimonials\" \r\n                 :key=\"testimonial.name\" \r\n                 class=\"testimonial-card\">\r\n              <div class=\"testimonial-content\">\r\n                <p class=\"testimonial-text\">{{ testimonial.content }}</p>\r\n              </div>\r\n              <div class=\"testimonial-author\">\r\n                <div class=\"author-avatar\">\r\n                  {{ testimonial.name.charAt(0) }}\r\n                </div>\r\n                <div class=\"author-info\">\r\n                  <div class=\"author-name\">{{ testimonial.name }}</div>\r\n                  <div class=\"author-title\">{{ testimonial.title }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"testimonials-nav\">\r\n            <button v-for=\"(_, index) in profile.testimonials\" \r\n                    :key=\"index\"\r\n                    class=\"nav-dot\"\r\n                    :class=\"{ active: currentTestimonial === index }\"\r\n                    @click=\"setTestimonial(index)\">\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, onUnmounted } from 'vue'\r\nimport axios from 'axios'\r\nimport { API_URLS } from '../config/api'\r\n\r\nexport default {\r\n  name: 'HomePage',\r\n  setup() {\r\n    const profile = ref({\r\n      title: '',\r\n      subtitle: '',\r\n      skills: [],\r\n      qualities: [],\r\n      testimonials: []\r\n    })\r\n    const featuredProjects = ref([])\r\n    const currentTestimonial = ref(0)\r\n    const autoScrollInterval = ref(null)\r\n\r\n    const getSkillIcon = (skill) => {\r\n      const icons = {\r\n        'JavaScript': 'fab fa-js',\r\n        'HTML5': 'fab fa-html5',\r\n        'CSS3': 'fab fa-css3-alt',\r\n        'React': 'fab fa-react',\r\n        'Vue.js': 'fab fa-vuejs',\r\n        'GitHub': 'fab fa-github',\r\n        'Java': 'fab fa-java',\r\n        // 添加更多图标映射\r\n      }\r\n      return icons[skill] || 'fas fa-code'\r\n    }\r\n\r\n    const setTestimonial = (index) => {\r\n      currentTestimonial.value = index\r\n    }\r\n\r\n    const startAutoScroll = () => {\r\n      autoScrollInterval.value = setInterval(() => {\r\n        currentTestimonial.value = (currentTestimonial.value + 1) % profile.value.testimonials.length\r\n      }, 5000)\r\n    }\r\n\r\n    const stopAutoScroll = () => {\r\n      if (autoScrollInterval.value) {\r\n        clearInterval(autoScrollInterval.value)\r\n      }\r\n    }\r\n\r\n    const fetchData = async () => {\r\n      try {\r\n        const [profileRes, projectsRes] = await Promise.all([\r\n          axios.get(API_URLS.PROFILE),\r\n          axios.get(`${API_URLS.PROJECTS}?filter=featured`)\r\n        ])\r\n\r\n        console.log('Profile API response:', profileRes.data)\r\n        console.log('Projects API response:', projectsRes.data)\r\n\r\n        if (profileRes.data) {\r\n          profile.value = profileRes.data\r\n        }\r\n\r\n        if (projectsRes.data && projectsRes.data.projects) {\r\n          featuredProjects.value = projectsRes.data.projects\r\n        } else if (projectsRes.data && Array.isArray(projectsRes.data.featured)) {\r\n          featuredProjects.value = projectsRes.data.featured\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching data:', error)\r\n      }\r\n    }\r\n\r\n    onMounted(() => {\r\n      fetchData()\r\n      startAutoScroll()\r\n    })\r\n\r\n    onUnmounted(() => {\r\n      stopAutoScroll()\r\n    })\r\n\r\n    return {\r\n      profile,\r\n      featuredProjects,\r\n      currentTestimonial,\r\n      getSkillIcon,\r\n      setTestimonial\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.hero {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 6rem 0;\r\n}\r\n\r\n.hero .container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.hero-content {\r\n  max-width: 600px;\r\n}\r\n\r\n.hero-image {\r\n  max-width: 400px;\r\n}\r\n\r\n.placeholder-image {\r\n  width: 100%;\r\n  height: 300px;\r\n  background-color: #112240;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border-radius: 8px;\r\n  color: #64ffda;\r\n  font-size: 1.2rem;\r\n  text-align: center;\r\n  padding: 2rem;\r\n}\r\n\r\n.hero-image img {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.availability {\r\n  display: inline-block;\r\n  padding: 0.5rem 1rem;\r\n  background-color: rgba(100, 255, 218, 0.1);\r\n  color: #64ffda;\r\n  border-radius: 4px;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.hero-title {\r\n  font-size: 4rem;\r\n  margin-bottom: 1.5rem;\r\n  color: #e6f1ff;\r\n}\r\n\r\n.hero-subtitle {\r\n  font-size: 1.25rem;\r\n  color: #8892b0;\r\n  margin-bottom: 2rem;\r\n  max-width: 600px;\r\n}\r\n\r\n.hero-buttons {\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n\r\n.section-subtitle {\r\n  font-size: 1.75rem;\r\n  margin-bottom: 1rem;\r\n  color: #64ffda;\r\n}\r\n\r\n.section-description {\r\n  font-size: 1.1rem;\r\n  color: #8892b0;\r\n  margin-bottom: 3rem;\r\n  max-width: 600px;\r\n}\r\n\r\n.projects {\r\n  background-color: #0a192f;\r\n}\r\n\r\n.project-card {\r\n  overflow: hidden;\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 100%;\r\n}\r\n\r\n.project-year {\r\n  font-size: 0.9rem;\r\n  color: #64ffda;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.project-divider {\r\n  height: 1px;\r\n  background-color: #233554;\r\n  margin: 1rem 0;\r\n}\r\n\r\n.project-title {\r\n  font-size: 1.5rem;\r\n  margin-bottom: 1rem;\r\n  color: #e6f1ff;\r\n}\r\n\r\n.project-description {\r\n  color: #8892b0;\r\n  margin-bottom: 1.5rem;\r\n  flex-grow: 1;\r\n}\r\n\r\n.project-tags {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 0.5rem;\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\n.tag {\r\n  padding: 0.25rem 0.75rem;\r\n  background-color: #233554;\r\n  color: #64ffda;\r\n  border-radius: 20px;\r\n  font-size: 0.875rem;\r\n}\r\n\r\n.project-links {\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n\r\n.project-link {\r\n  color: #8892b0;\r\n  font-size: 1.25rem;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.project-link:hover {\r\n  color: #64ffda;\r\n}\r\n\r\n.skills {\r\n  background-color: #112240;\r\n  padding: 3rem 0;\r\n  overflow: hidden;\r\n}\r\n\r\n.skills-slider {\r\n  width: 100%;\r\n  overflow: hidden;\r\n}\r\n\r\n.skills-track {\r\n  display: flex;\r\n  animation: scroll 30s linear infinite;\r\n}\r\n\r\n.skill-item {\r\n  white-space: nowrap;\r\n  padding: 1rem 2rem;\r\n  color: #e6f1ff;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n@keyframes scroll {\r\n  0% {\r\n    transform: translateX(0);\r\n  }\r\n  100% {\r\n    transform: translateX(-50%);\r\n  }\r\n}\r\n\r\n.testimonials {\r\n  background-color: #0a192f;\r\n}\r\n\r\n.testimonial-card {\r\n  background-color: #112240;\r\n  border-radius: 4px;\r\n  padding: 2rem;\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.testimonial-text {\r\n  font-size: 1.2rem;\r\n  color: #8892b0;\r\n  margin-bottom: 2rem;\r\n  font-style: italic;\r\n}\r\n\r\n.testimonial-author {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-start;\r\n}\r\n\r\n.author-name {\r\n  font-size: 1.1rem;\r\n  color: #e6f1ff;\r\n  font-weight: 600;\r\n}\r\n\r\n.author-title {\r\n  font-size: 0.9rem;\r\n  color: #64ffda;\r\n}\r\n\r\n.cta {\r\n  background-color: #112240;\r\n  text-align: center;\r\n}\r\n\r